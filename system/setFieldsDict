FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


regions
(
    boxToCell
    {
        box (-1 -1 -1) (-0.01 1 1);

        fieldValues
        (
            volScalarFieldValue alpha.water 0.99999999
        );
    }
);




